<?xml version="1.0" encoding="UTF-8"?>

<!--IMPORTANT: This configuration applies to developer tests/runs only. The 'real' configuration is src/dist/log/log4j2.xml-->
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} %-5level- %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>